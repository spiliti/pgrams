<!DOCTYPE hibernate-mapping PUBLIC
   "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
   "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class
		name="org.egov.ptis.domain.entity.property.MutationRegistrationDetails"
		table="EGPT_MUTATION_REGISTRATION_DETAILS">
		<id name="id" column="ID" type="java.lang.Long">
			<generator class="org.hibernate.id.enhanced.SequenceStyleGenerator">
				<param name="sequence_name">SEQ_EGPT_MUTATION_REGISTRATION_DETAILS</param>
			</generator>
		</id>

		<property name="seller" column="SELLER" />
		<property name="buyer" column="BUYER" />
		<property name="typeOfTransfer" column="TYPEOFTRANSFER" />
		<property name="documentNo" column="DOCUMENTNO" />
		<property name="documentDate" column="DOCUMENTDATE" />
		<property name="partyValue" column="PARTYVALUE" />
		<property name="departmentValue" column="DEPARTMENTVALUE" />
		<property name="doorNo" column="DOORNO" />
		<property name="address" column="ADDRESS" />
		<property name="eastBoundary" column="EASTBOUNDARY" />
		<property name="westBoundary" column="WESTBOUNDARY" />
		<property name="northBoundary" column="NORTHBOUNDARY" />
		<property name="southBoundary" column="SOUTHBOUNDARY" />
		<property name="plinthArea" column="PLINTHAREA" />
		<property name="plotArea" column="PLOTAREA" />
		<property name="sroName" column="SRONAME"></property>
		<property name="documentValue" column="DOCUMENTVALUE"></property>
		<property name="documentLink" column="DOCUMENTLINK"></property>
		<property name="remarks" column="remarks"></property>
		
	</class>
</hibernate-mapping>
