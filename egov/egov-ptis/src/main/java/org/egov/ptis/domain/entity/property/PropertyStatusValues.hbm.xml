<!DOCTYPE hibernate-mapping PUBLIC
   "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
   "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="org.egov.ptis.domain.entity.property.PropertyStatusValues"
		table="EGPT_PROPERTY_STATUS_VALUES">
		<id name="id" column="ID" type="java.lang.Long">
			<generator class="org.hibernate.id.enhanced.SequenceStyleGenerator">
				<param name="sequence_name">SEQ_PROPERTY_STATUS_VALUES</param>
			</generator>
		</id>
		<property name="createdDate" type="java.util.Date">
			<column name="CREATED_DATE" not-null="true" />
		</property>
		<property name="modifiedDate" type="java.util.Date">
			<column name="MODIFIED_DATE" not-null="true" />
		</property>
		<many-to-one name="createdBy" class="org.egov.infra.admin.master.entity.User"
			cascade="all">
			<column name="CREATED_BY" />
		</many-to-one>
		<many-to-one name="modifiedBy" class="org.egov.infra.admin.master.entity.User"
			cascade="all">
			<column name="MODIFIED_BY" />
		</many-to-one>

		<many-to-one name="basicProperty" column="ID_BASIC_PROPERTY"
			class="org.egov.ptis.domain.entity.property.BasicPropertyImpl" insert="false"
			update="false" />

		<many-to-one name="propertyStatus" column="ID_STATUS"
			class="org.egov.ptis.domain.entity.property.PropertyStatus" />


		<property name="referenceDate" column="REF_DATE" />
		<property name="referenceNo" column="REF_NUM" />
		<property name="remarks" column="REMARKS" />
		<property name="isActive" column="IS_ACTIVE" />
		<property name="extraField1" column="EXTRA_FIELD1" />
		<property name="extraField2" column="EXTRA_FIELD2" />
		<property name="extraField3" column="EXTRA_FIELD3" />

		<many-to-one name="referenceBasicProperty" column="REF_ID_BASIC"
			class="org.egov.ptis.domain.entity.property.BasicPropertyImpl" />

	</class>
	
</hibernate-mapping>
